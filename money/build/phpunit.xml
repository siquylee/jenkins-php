<?xml version="1.0" encoding="utf-8"?>
<phpunit xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance" xsi:noNamespaceSchemaLocation="https://schema.phpunit.de/5.2/phpunit.xsd" bootstrap="../src/autoload.php" backupGlobals="false" backupStaticAttributes="false" beStrictAboutCoversAnnotation="true" beStrictAboutOutputDuringTests="true" beStrictAboutTestsThatDoNotTestAnything="true" beStrictAboutTodoAnnotatedTests="true" forceCoversAnnotation="true" verbose="true">
    <testsuite name="Money">
        <directory suffix="Test.php">../tests</directory>
    </testsuite>
